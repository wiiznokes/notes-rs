{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"label": "build",
			"command": "build",
			"args": [
				"--release"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		},
		{
			"type": "cargo",
			"label": "clippy",
			"command": "clippy",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		},
		{
			"type": "cargo",
			"label": "clean",
			"command": "clean",
			"problemMatcher": [
				"$rustc"
			],
			"group": "clean"
		}
	]
}